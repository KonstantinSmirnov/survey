<%= form_for @survey do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, autocomplete: 'off', class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, autocomplete: 'off', class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
  </div>
<% end %>
